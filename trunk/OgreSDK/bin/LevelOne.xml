    </Body>
    <Body>
      <BodyDef/>
      <Fixture shape="edge" point1="-20 0" point2="20 0"/>
    </Body>
    <Body id="MySensor">
      <BodyDef bodyType="dynamicBody" position="15 20"/>
      <Fixture shape="box" boxWidth="1" boxHeight="1" density="40"/>
    </Body>
    <Body id="ground">
      <BodyDef position="0 -50"/>
      <Fixture shape="box" boxWidth="1" boxHeight="1" density="40"/>
    </Body>
    <Body>
      <BodyDef position="-10.6 2.5"/>
      <Fixture shape="box" boxWidth="1.2" boxHeight="5.3" density="40"/>
    </Body>
    <Body>
      <BodyDef position="-20.6 2.5"/>
      <Fixture shape="box" boxWidth="1.2" boxHeight="5.3" density="40"/>
    </Body>
    <Body>
      <BodyDef position="-10.6 7.5"/>
      <Fixture shape="box" boxWidth="1.2" boxHeight="5.3" density="40"/>
    </Body>
    <Body>
      <BodyDef position="-20.6 7.5"/>
      <Fixture shape="box" boxWidth="1.2" boxHeight="5.3" density="40"/>
    </Body>
    <Body id="LevarArm1">
      <BodyDef bodyType="dynamicBody" position="35 2"/>
      <Fixture shape="box" boxWidth="14.16" boxHeight="1.2" density="40"/>
    </Body>
    <Body id="LevarArm2">
      <BodyDef bodyType="dynamicBody" position="50 -4"/>
      <Fixture shape="box" boxWidth="14.16" boxHeight="1.2" density="40"/>
    </Body>
    <Joint id="Revjoint" type="revolute">
      <JointDef bodyA="ground" bodyB="LevarArm1"/>
    </Joint>
    <Joint id="Revjoint" type="revolute">
      <JointDef bodyA="ground" bodyB="LevarArm2"/>
    </Joint>
    <Body>
      <BodyDef/>
      <Fixture shape="edge" point1="75 -1" point2="90 -1"/>
    </Body>
    <Body>
      <BodyDef/>
      <Fixture shape="edge" point1="90 -1" point2="120 -10"/>
    </Body>
    <Body>
      <BodyDef/>
      <Fixture shape="edge" point1="120 -10" point2="135 -10"/>
    </Body>
    <Body id="steve">
      <BodyDef bodyType="dynamicBody" position="137 -10"/>
      <Fixture shape="box" boxWidth="3" boxHeight="0.5" density="15"/>
    </Body>
    <Joint id="MrJoint" type="prismatic">
      <JointDef bodyA="ground" bodyB="steve" localAxis1="0 1" enableLimit="1" lowerTranslation="-20" upperTranslation="1" worldAnchor="137 -10" enableMotor="1" motorSpeed="200" maxMotorForce="1500"/>
      <!--<JointDef bodyA="bill" bodyB="steve" localAnchorA="0 10" localAnchorB="3 2" groundAnchorA="0 11" groundAnchorB="3 11" ratio="2" lengthB="5"/>-->
    </Joint>
    <Body>
      <BodyDef/>
      <Fixture shape="edge" point1="100 -31" point2="135 -31" />
    </Body>
  </Box2D>
  <GameObjects>
    <GameObject type="OgreBox2D">
      <Object name="A lever Arm"/>
      <OgreObject position="0 0 0" >
        <Mesh mesh="LeverArm.mesh" scale="0.4 0.2 0.2"/>
      </OgreObject>
      <Box2DObject bodyID="LevarArm1"/>
    </GameObject>
    <GameObject type="OgreBox2D">
