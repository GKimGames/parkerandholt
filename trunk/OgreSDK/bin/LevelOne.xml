    <GameObject type="ObjectSensor">
      <Object name="sleeves"/>
      <OgreObject position="0 0 0" >
        <Mesh mesh="cube.1m.mesh"/>
      </OgreObject>
      <Box2DObject bodyID="MySensor"/>
      <ObjectSensor messageOnContact="1">
        <Object objectID="MyObjectSensor" on=""/>
        <Messages>
          <Message id="Jumper" type="" delay="0">
            <data>
              <kittycat butts="butts"/>
            </data>
          </Message>
        </Messages>
      </ObjectSensor>
    </GameObject>
    <GameObject type="ObjectSensor">
      <Object name="MyLedgeSensor"/>
      <Box2DObject bodyID="myLedge4"/>
      <ObjectSensor>
        <Object objectID="MyObjectSensor" sensorType="sensortype ledge"/>
      </ObjectSensor>
    </GameObject>
    <GameObject type="ObjectSensor">
      <Object name="MyLedgeSensor"/>
      <Box2DObject bodyID="myLedge2"/>
      <ObjectSensor>
        <Object objectID="MyObjectSensor" sensorType="sensortype ledge"/>
