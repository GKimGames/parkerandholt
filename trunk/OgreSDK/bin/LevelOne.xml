      <Box2DObject bodyID="farOffLedge"/>
      <ObjectSensor>
        <Object objectID="MyObjectSensor" sensorType="sensortype ledge"/>
      </ObjectSensor>
    </GameObject>
    <GameObject type="OgreBox2D">
